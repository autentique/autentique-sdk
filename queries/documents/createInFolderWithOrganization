"mutation CreateDocumentMutation(
  $document: DocumentInput!,
  $signers: [SignerInput!]!,
  $file: Upload!,
  $sandbox: Boolean!,
  $organizationId: Int!,
  $folder: UUID!,
) {
  createDocument(
    sandbox: $sandbox,
    document: $document,
    signers: $signers,
    file: $file,
    organization_id: $organizationId,
    folder_id: $folder
  ) {
    id
    name
    refusable
    sortable
    created_at
    signatures {
      public_id
      name
      email
      created_at
      action { name }
      link { short_link }
      user { id name email }
    }
  }
}",
"variables": $variables